@model IEnumerable<Member>

@{
    ViewBag.Title = "Lista de miembros";
}

<div class="card-deck">
    @foreach (var member in Model)
    {
        <div class="card m-3">
            <div class="card-header">
                <h3>@member.Name</h3>
            </div>
            <img class="card-img-top" src="~/images/circleCode.jpg" asp-append-version="true" />
            <div class="card-footer text-center">
                <a asp-controller="Main" asp-action="Details" asp-route-id="@member.Id" class="btn btn-primary">Ver</a>
                <a href="#" class="btn btn-primary">Modificar</a>
                <a href="#" class="btn btn-primary">Eliminar</a>
            </div>
        </div>
    }
</div>